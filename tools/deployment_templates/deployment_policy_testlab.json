{
  "properties": [ ],
  "constraints": [ 
    "zone == \"blue\""
  ],
  "service": {
    "org": "$HZN_ORG_ID",
    "name": "$SERVICE_NAME",
    "arch": "*",
    "serviceVersions": [ { "version": "$SERVICE_VERSION", "priority": {} } ]    
  },
  "userInput": [
    {
      "serviceOrgid": "$HZN_ORG_ID",
      "serviceUrl": "$SERVICE_NAME",
      "serviceVersionRange": "[1.0.INFINITY)"
    }
  ]
}

