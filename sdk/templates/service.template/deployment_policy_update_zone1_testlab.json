{
  "properties": [ ],
  "constraints": [
    "zone_id == \"zone1\"",
    "env == \"testlab\""
  ],
  "service": {
    "org": "$HZN_ORG_ID",
    "name": "$SERVICE_NAME",
    "arch": "*",
    "serviceVersions": [ { "version": "$SERVICE_VERSION", "priority": {} } ]    
  },
  "userInput": [
    {
      "serviceOrgid": "$HZN_ORG_ID",
      "serviceUrl": "$SERVICE_NAME",
      "serviceVersionRange": "[1.0.INFINITY)",
      "inputs": [
        {
          "name": "HW_ID", "value": "hw.zone1"
        }
      ]
    }
  ]
}

